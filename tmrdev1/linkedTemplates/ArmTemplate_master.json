{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tmrdev1"
		},
		"azsqlReportingDBconn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azsqlReportingDBconn'"
		},
		"lksCabChargeBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lksCabChargeBlobStorage'"
		},
		"lksTmrFileStorage_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lksTmrFileStorage'"
		},
		"lksTmrFileStorage_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\tmrdevstorage.file.core.windows.net\\cabcharge"
		},
		"lksTmrFileStorage_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Azure\\tmrdevstorage"
		},
		"tmrKeydev_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://tmr-key.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tmrdev1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lksCabChargeBlobStorage_connectionString": {
						"value": "[parameters('lksCabChargeBlobStorage_connectionString')]"
					},
					"lksTmrFileStorage_password": {
						"value": "[parameters('lksTmrFileStorage_password')]"
					},
					"azsqlReportingDBconn_connectionString": {
						"value": "[parameters('azsqlReportingDBconn_connectionString')]"
					},
					"lksTmrFileStorage_properties_typeProperties_host": {
						"value": "[parameters('lksTmrFileStorage_properties_typeProperties_host')]"
					},
					"lksTmrFileStorage_properties_typeProperties_userId": {
						"value": "[parameters('lksTmrFileStorage_properties_typeProperties_userId')]"
					},
					"tmrKeydev_properties_typeProperties_baseUrl": {
						"value": "[parameters('tmrKeydev_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}